extends ../layout/main
block content
	section.content-header
		h1 Update Product -  #{product.title}
	section.content
		div.row
			div.col-md-12
				div.nav-tabs-custom
					ul.nav.nav-tabs
						li.active
							a(href="/backoffice/product/update/"+product.key) Info
						li
							a(href="/backoffice/product/image/"+product.key) Image
					div.tab-content
						include form